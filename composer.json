{
  "name": "ama-team/elasticsearch-schema",
  "description": "PHP Bindings for ElasticSearch schema",
  "license": "MIT",
  "authors": [
    {
      "name": "AMA Team",
      "email": "dev@amagroup.ru"
    },
    {
      "name": "Etki",
      "email": "etki@etki.me"
    }
  ],
  "require": {
    "php": ">= 7.1",
    "ext-json": "*",
    "ama-team/elasticsearch-schema-definitions": "dev-dev",
    "symfony/validator": ">= 3.0.0, < 5.0.0",
    "league/flysystem": "^1.0",
    "jms/serializer": "^1.10",
    "opis/json-schema": "^1.0",
    "remorhaz/php-json-pointer": "^0.4.0"
  },
  "require-dev": {
    "codeception/codeception": "^2.3",
    "allure-framework/allure-codeception": "^1.2",
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^3.2",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "AmaTeam\\ElasticSearch\\Schema\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AmaTeam\\ElasticSearch\\Schema\\Test\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
